{
  "name": "Swift",
  "dockerFile": "Dockerfile",
  "runArgs": [
    "--cap-add=SYS_PTRACE",
    "--security-opt",
    "seccomp=unconfined"
  ],
  "settings": {
    "lldb.adapterType": "bundled",
    "lldb.executable": "/usr/bin/lldb",
    "terminal.integrated.defaultProfile.linux": "/bin/bash",
  },
  "extensions": [
    "pvasek.sourcekit-lsp--dev-unofficial",
    "vadimcn.vscode-lldb",
  ],
}